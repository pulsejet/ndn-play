{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/dist/out-tsc/app/user-types.d.ts",
  "bundledPackages": [
    "@ndn/keychain",
    "@ndn/packet",
    "@ndn/tlv",
    "@ndn/ws-transport",
    "@ndn/autoconfig",
    "@ndn/l3face",
    "@ndn/lp",
    "@ndn/endpoint",
    "@ndn/fw",
    "@ndn/util",
    "@ndn/repo-api",
    "@ndn/svs",
    "@ndn/psync",
    "@ndn/sync-api",
    "@ndn/trust-schema",
    "@yoursunny/psync-bloom",
    "wait-your-turn",
    "@angular/core",
    "rxjs",
    "vis-network",
    "typed-emitter",
    "abort-controller",
    "event-target-shim",
    "type-fest",
    "streaming-iterables",
    "typescript-event-target"
  ],
  "compiler": {},
  "apiReport": { "enabled": false },
  "docModel": { "enabled": false },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/src/assets/user-types.d.ts"
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "newlineKind": "crlf",
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
